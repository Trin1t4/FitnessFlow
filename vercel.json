{
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "devCommand": "cd client && npm run dev",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
 {
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  }
}

    }
  }
}
